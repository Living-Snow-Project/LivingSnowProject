{
  "expo": {
    "name": "Living Snow Project",
    "slug": "LivingSnowProject",
    "privacy": "unlisted",
    "primaryColor": "#000000",
    "platforms": ["ios", "android"],
    "version": "2.3.3",
    "orientation": "portrait",
    "icon": "./assets/images/lsp@2x.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ef724170-99df-11e9-80a2-03e7acec1afa"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kodnerlab.livingsnowproject",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Living Snow Project to determine your location for easier snow algae reporting",
        "NSPhotoLibraryUsageDescription": "Allow Living Snow Project to access your photos and supplement snow algae reporting"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.livingsnowproject.livingsnowproject",
      "versionCode": 3,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "e9398179-0d31-4623-8b30-84ca256f8a71"
      }
    },
    "plugins": ["expo-font"]
  }
}
